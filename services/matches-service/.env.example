PORT=5009
NODE_ENV=development

# DB (recommended to use the same cluster/DB as booking-service if you already have buddy data there)
MONGO_URI=<MONGODB_ATLAS_OR_LOCAL_URI>

# Must match auth-service JWT secret
JWT_SECRET=<SAME_AS_AUTH_SERVICE_JWT_SECRET>

# Used to validate token via Gateway -> /api/v1/auth/me
GATEWAY_URL=http://localhost:5050
